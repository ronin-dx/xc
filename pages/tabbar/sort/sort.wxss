page {
  background-color: #EDEDED;
}

.items {
  width: 700rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 14rpx;
  background-color: #ffffff;
  padding-bottom: 20rpx;
  box-shadow: 0 4rpx 24rpx 0 rgba(0, 0, 0, 0.1);
}

.items .title {
  width: 650rpx;
  display: flex;
  margin-left: 50rpx;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #eeeeee;
  line-height: 100rpx;
}
.items .title .name {
  font-size: 36rpx;
}
.items .title .icon {
  margin-right: 30rpx;
  color: #cccccc;
}
.items .content {
  width: 650rpx;
  display: flex;
  flex-wrap: wrap;
  margin-left: 50rpx;
  margin-top: 10rpx;
}
.items .content view {
  width: 50%;
  line-height: 80rpx;
}
.items:last-child {
  margin-bottom: 30rpx;
}